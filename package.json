		"multer": "^1.4.1",
		"react": "^16.4.2",
		"react-dom": "^16.4.2",
		"react-redux": "^5.1.0",
		"react-router": "^4.3.1",
		"react-router-dom": "^4.3.1",
		"react-router-redux": "^4.0.8",
