{
	"name": "kernelization",
	"version": "0.2.0",
	"description": "The best discord server ever!",
	"private": true,
	"main": "index.js",
	"scripts": {
		"frontend": "webpack-dev-server --config ./webpack.config.js --mode development",
		"backend": "nodemon backend/server.js",
		"dev": "concurrently \" npm run frontend\" \" npm run backend\"",
		"prod": "webpack --mode production --watch",
		"build": "cross-env NODE_ENV=production webpack --config prod.config.js",
		"clean": "rimraf dist",
		"wipe": "rimraf node_modules && npm install",
		"count": "git ls-files -- . ':!:*.json' --exclude-standard | xargs wc -l"
	},
	"author": "Brett Broadhurst <brettbroadhurst@gmail.com>, Ankit Kumar Tyagi <ankit.tyagiw@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/thehighgod/Kernelization.git"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.5",
		"babel-preset-env": "^1.7.0",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"chunk-manifest-webpack-plugin": "^1.1.2",
		"concurrently": "^3.6.1",
		"cross-env": "^5.2.0",
		"css-loader": "^1.0.0",
		"cssnano": "^4.1.7",
		"eslint": "^5.9.0",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^2.0.0",
		"html-webpack-plugin": "^3.2.0",
		"mini-css-extract-plugin": "^0.4.2",
		"morgan": "^1.9.1",
		"node-sass": "^4.9.3",
		"nodemon": "^1.18.3",
		"optimize-css-assets-webpack-plugin": "^5.0.1",
		"postcss-cssnext": "^3.1.0",
		"postcss-focus": "^4.0.0",
		"postcss-reporter": "^6.0.0",
		"redux-logger": "^3.0.6",
		"rimraf": "^2.6.2",
		"sass-loader": "^7.1.0",
		"scss": "^0.2.4",
		"style-loader": "^0.22.1",
		"uglifyjs-webpack-plugin": "^2.0.1",
		"url-loader": "^1.1.1",
